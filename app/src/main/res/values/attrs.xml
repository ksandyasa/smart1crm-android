<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <attr name="customFont" format="string"/>
    </declare-styleable>
    <declare-styleable name="CustomEditText">
        <attr name="customEditTextFont" format="string"/>
    </declare-styleable>
    <declare-styleable name="CustomButton">
        <attr name="customButtonFont" format="string"/>
    </declare-styleable>
    <declare-styleable name="CustomSpinner">
        <attr name="customSpinnerFont" format="string"/>
    </declare-styleable>
    <!-- Round ImageView -->
    <declare-styleable name="RoundedImageView">
        <attr name="corner_radius" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="round_background" format="boolean" />
        <attr name="is_oval" format="boolean" />
        <attr name="android:scaleType" />
    </declare-styleable>
</resources>